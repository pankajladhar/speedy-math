  "private": true,
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "react-front-cli": "^1.1.3",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^16.4.1",
    "run-git-command": "^1.2.1"
  },
  "dependencies": {
    "react": "^16.4.1",
